<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"  
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="cacheEnabled" value="true" /><!-- 全局映射器启用缓存 -->
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="logImpl" value="LOG4J" />
	</settings>

	<typeAliases>
		<typeAlias type="com.fh.entity.system.User" alias="User" />
		<typeAlias type="com.fh.entity.system.Role" alias="Role" />
		<typeAlias type="com.fh.entity.system.Menu" alias="Menu" />
		<typeAlias type="com.fh.util.PageData" alias="pd" />
		<!-- 分页 -->
		<typeAlias type="com.fh.entity.Page" alias="Page" />
		<typeAlias type="com.fh.entity.BusinessManInfo" alias="BusinessManInfo" />
		<typeAlias type="com.fh.entity.ProductInfo" alias="ProductInfo" />
		<typeAlias type="com.fh.entity.ShoppingCartInfo" alias="ShoppingCartInfo" />
		<typeAlias type="com.fh.entity.Dictionary" alias="Dictionary" />
		<typeAlias type="com.fh.entity.ManImage" alias="ManImage" />

		<typeAlias type="com.fh.entity.MainType" alias="MainType" />
		<typeAlias type="com.fh.entity.OrderDetailInfo" alias="OrderDetailInfo" />
		<typeAlias type="com.fh.entity.OrderInfo" alias="OrderInfo" />
		<typeAlias type="com.fh.entity.GradeInfo" alias="GradeInfo" />
		<typeAlias type="com.fh.entity.MainTypeInfo" alias="MainTypeInfo" />

		<typeAlias type="com.fh.entity.Address" alias="Address" />


	</typeAliases>

	<plugins>
		<plugin interceptor="com.fh.plugin.PagePlugin">
			<property name="dialect" value="mysql" />
			<property name="pageSqlId" value=".*listPage.*" />
		</plugin>
	</plugins>

</configuration>